<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="loginPane" VBox.vgrow="ALWAYS" fx:controller="org.kumoricon.staff.client.loginscreen.LoginPresenter"  xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1" >
    <children>
        <TextField fx:id="txtUsername" alignment="CENTER" layoutX="251.0" layoutY="66.0" prefHeight="26.0" prefWidth="370.0" promptText="Username" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="66.0" />
        <PasswordField fx:id="txtPassword" alignment="CENTER" layoutX="251.0" layoutY="109.0" prefHeight="26.0" prefWidth="370.0" promptText="Password" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="109.0" />
        <TextField fx:id="txtServerURL" alignment="CENTER" layoutX="251.0" layoutY="160.0" prefHeight="26.0" prefWidth="370.0" promptText="Server URL" text="http://localhost:8080" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="160.0" />
        <Button fx:id="btnLogin" defaultButton="true" layoutX="562.0" layoutY="209.0" mnemonicParsing="false" onAction="#loginClicked" text="Login" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="209.0" />
        <Button fx:id="btnCancel" cancelButton="true" layoutX="484.0" layoutY="209.0" mnemonicParsing="false" onAction="#quitClicked" text="Cancel" AnchorPane.rightAnchor="280.0" AnchorPane.topAnchor="209.0" />
    </children>
</AnchorPane>